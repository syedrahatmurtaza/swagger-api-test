{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "outputDirectory": "public",
    "routesDir": "temp",
    "specVersion": 3,
    "securityDefinitions": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "name": "authorization",
        "in": "header",
        "bearerFormat": "JWT"
      }
    }
  },
  "routes": {
    "basePath": "/",
    "routesDir": "./routes/tsoaRoutes",
    "middlewear": "express",
    "authenticationModule": "./routes/tsoaRoutes/authentication.ts"
  }
}
